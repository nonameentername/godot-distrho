@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix midi:  <http://lv2plug.in/ns/ext/midi#> .
@prefix mod:   <http://moddevices.com/ns/mod#> .
@prefix opts:  <http://lv2plug.in/ns/ext/options#> .
@prefix pg:    <http://lv2plug.in/ns/ext/port-groups#> .
@prefix patch: <http://lv2plug.in/ns/ext/patch#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rsz:   <http://lv2plug.in/ns/ext/resize-port#> .
@prefix spdx:  <http://spdx.org/rdf/terms#> .
@prefix ui:    <http://lv2plug.in/ns/extensions/ui#> .
@prefix unit:  <http://lv2plug.in/ns/extensions/units#> .

<https://github.com/nonameentername/godot-distrho>
    a lv2:SimulatorPlugin, lv2:Plugin, doap:Project ;

    lv2:extensionData opts:interface ;

    lv2:optionalFeature <http://lv2plug.in/ns/lv2core#hardRTCapable> ,
                        <http://lv2plug.in/ns/ext/buf-size#boundedBlockLength> ;

    lv2:requiredFeature opts:options ,
                        <http://lv2plug.in/ns/ext/urid#map> ;

    opts:supportedOption <http://lv2plug.in/ns/ext/buf-size#nominalBlockLength> ,
                         <http://lv2plug.in/ns/ext/buf-size#maxBlockLength> ,
                         <http://lv2plug.in/ns/ext/parameters#sampleRate> ;

    ui:ui <https://github.com/nonameentername/godot-distrho#DPF_UI> ;

    lv2:port [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "lv2_input_left" ;
        lv2:name "Input Left" ;
        pg:group <https://github.com/nonameentername/godot-distrho#portGroup_dpf_stereo> ;
        lv2:designation pg:left ;
    ] ,
    [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 1 ;
        lv2:symbol "lv2_input_right" ;
        lv2:name "Input Right" ;
        pg:group <https://github.com/nonameentername/godot-distrho#portGroup_dpf_stereo> ;
        lv2:designation pg:right ;
    ] ;

    lv2:port [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 2 ;
        lv2:symbol "lv2_output_left" ;
        lv2:name "Output Left" ;
        pg:group <https://github.com/nonameentername/godot-distrho#portGroup_dpf_stereo> ;
        lv2:designation pg:left ;
    ] ,
    [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 3 ;
        lv2:symbol "lv2_output_right" ;
        lv2:name "Output Right" ;
        pg:group <https://github.com/nonameentername/godot-distrho#portGroup_dpf_stereo> ;
        lv2:designation pg:right ;
    ] ;

    lv2:port [
        a lv2:InputPort, atom:AtomPort ;
        lv2:index 4 ;
        lv2:name "Events Input" ;
        lv2:symbol "lv2_events_in" ;
        rsz:minimumSize 2048 ;
        atom:bufferType atom:Sequence ;
        atom:supports midi:MidiEvent ;
    ] ;

    lv2:port [
        a lv2:OutputPort, atom:AtomPort ;
        lv2:index 5 ;
        lv2:name "Events Output" ;
        lv2:symbol "lv2_events_out" ;
        rsz:minimumSize 2048 ;
        atom:bufferType atom:Sequence ;
        atom:supports midi:MidiEvent ;
    ] ;

    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 6 ;
        lv2:name "Volume" ;
        lv2:symbol "V" ;
        lv2:default 0.5 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
        unit:unit unit:db ;
        rdfs:comment "Volume used to change how loud sound is played" ;
        lv2:portProperty <http://lv2plug.in/ns/ext/port-props#expensive> ,
                         <http://kxstudio.sf.net/ns/lv2ext/props#NonAutomatable> ;
        pg:group <https://github.com/nonameentername/godot-distrho#portGroup_> ;
    ] ;

    rdfs:comment "Godot DISTRHO plugin" ;

    mod:brand "GodotDistrho" ;
    mod:label "GodotDistrho" ;

    doap:name "GodotDistrho" ;
    doap:license <http://spdx.org/licenses/MIT.html> ;

    doap:maintainer [
        foaf:name "godot-distrho" ;
        foaf:homepage <https://github.com/nonameentername/godot-distrho> ;
    ] ;

    lv2:microVersion 1 ;
    lv2:minorVersion 0 .

<https://github.com/nonameentername/godot-distrho#portGroup_dpf_stereo>
    a pg:Group , pg:StereoGroup ;
    lv2:name "Stereo" ;
    lv2:symbol "dpf_stereo" .
