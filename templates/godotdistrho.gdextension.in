[godot-distrho]

version = "${version}"
build = "${build}"

[configuration]

entry_symbol = "godot_distrho_library_init"
compatibility_minimum = 4.3
reloadable = true

[libraries]

macos.debug = "res://addons/distrho/bin/macos/macos.framework/libdistrhogodot.macos.template_debug"
macos.release = "res://addons/distrho/bin/macos/macos.framework/libdistrhogodot.macos.template_release"
windows.debug.x86_64 = "res://addons/distrho/bin/windows/libdistrhogodot.windows.template_debug.dev.x86_64.dll"
windows.release.x86_64 = "res://addons/distrho/bin/windows/libdistrhogodot.windows.template_release.x86_64.dll"
linux.debug.x86_64 = "res://addons/distrho/bin/linux/libdistrhogodot.linux.template_debug.dev.x86_64.so"
linux.release.x86_64 = "res://addons/distrho/bin/linux/libdistrhogodot.linux.template_release.x86_64.so"
